{
    "name": "ccc/star-rating-bundle",
    "type": "symfony-bundle",
    "description": "Twig extension for quick integration of FyneWorks' Star Rating widget",
    "keywords": ["symfony", "fyneworks", "star-rating", "form", "twig"],
    "homepage": "http://github.com/cc-consulting/StarRatingBundle",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Evan Owens",
            "email": "evan.owens@ccconsulting.jp"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "fyneworks/star-rating",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/Fyneworks-jQuery/star-rating.git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "fyneworks/star-rating": "dev-master",
        "symfony/form": ">=2.2,<2.4-dev"
    },
    "autoload": {
        "psr-0": { "CCC\\StarRatingBundle": "" }
    },
    "target-dir": "CCC/StarRatingBundle",
    "minimum-stability": "dev"
}
